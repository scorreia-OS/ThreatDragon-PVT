            "id": "2a4eeda9-3962-4e18-b9dd-7821360dac2c",
            "data": {
              "type": "tm.Actor",
              "name": "Outsystems\nServer",
              "description": "Server with local users created. Provide Sudo access to Reliaquest users",
              "outOfScope": false,
              "reasonOutOfScope": "",
              "hasOpenThreats": true,
              "providesAuthentication": true,
              "threats": [
                {
                  "id": "401c715f-ea89-414b-9479-81bfda756cf0",
                  "title": "Lack of accountability",
                  "status": "Mitigated",
                  "severity": "High",
                  "type": "Repudiation",
                  "description": "Due to the use of shared accounts or poor logging, it's impossible to definitively determine which specific individual performed actions on the server using the third-party credentials.\nThreat Actor: Any user (legitimate, former, or attacker) using a shared or improperly attributed credential.\nImpact: Inability to perform audits or forensic analysis; undermines non-repudiation; hinders incident response and remediation.",
                  "mitigation": "Enforce unique, named user accounts. Implement comprehensive server-side logging and auditing of all actions performed by authenticated users. Logs and alerts are monitored constantly.",
                  "modelType": "STRIDE",
                  "new": false,
                  "number": 4,
                  "score": "7"
                },
                {
                  "id": "0d885376-8f4e-4b96-b2e3-8f3793b68440",
                  "title": "Over-Privileged Access",
                  "status": "Open",
                  "severity": "High",
                  "type": "Elevation of privilege",
                  "description": "The account used by the third party is granted more permissions (read, write, execute) on the server than are strictly necessary for their intended function, violating the principle of least privilege.\nThreat Actor: Malicious/Negligent Third-Party User, Attacker who has compromised the third-party account.\nImpact: Excessive data exposure/theft/modification; potential for privilege escalation or lateral movement; increased damage (\"blast radius\") from a compromised account.",
                  "mitigation": "Adhere strictly to the Principle of Least Privilege; use Role-Based Access Control (RBAC); conduct periodic permission reviews. Log monitoring and alerts for potential anomalous behaviour.",
                  "modelType": "STRIDE",
                  "new": false,
                  "number": 5,
                  "score": "8"
                },
                {
                  "id": "e9f6bfb5-bb12-4224-ad82-8547adaed790",
                  "title": "Default/Shared Account Usage",
                  "status": "Open",
                  "severity": "High",
                  "type": "Repudiation",
                  "description": "Using generic accounts not tied to individuals (e.g., \"vendor_login\") or failing to change default passwords set by software/hardware vendors connected to the server.\nThreat Actor: Opportunistic Attacker (targeting defaults), Any user of a shared account.\nImpact: Easy unauthorized access via known default credentials; complete lack of accountability for actions taken with shared accounts.",
                  "mitigation": "Prohibit shared accounts; enforce changing of all default credentials during setup; verify configurations.",
                  "modelType": "STRIDE",
                  "new": false,
                  "number": 6,
                  "score": "8"
                },
                {
                  "id": "261f580d-9aff-46ee-adec-6aabb1d06fe4",
                  "title": "Failure to Revoke Access Promptly",
                  "status": "Open",
                  "severity": "High",
                  "type": "Spoofing",
                  "description": "The third party's access credentials remain active on your server even after the business relationship has ended or the specific employee needing access has left the third-party company (orphaned account).\nThreat Actor: Former Third-Party Employee, External Attacker discovering/exploiting an orphaned account.\nImpact: Continued unauthorized access after the legitimate need has expired; potential data breach or sabotage by disgruntled ex-employees.",
                  "mitigation": "Implement formal access review and de-provisioning processes; conduct regular account audits; automate revocation where possible.",
                  "modelType": "STRIDE",
                  "new": false,
                  "number": 7,
                  "score": "7"
                }
              ],
              "threatFrequency": {
                "spoofing": 1,
                "repudiation": 2
              }
            }
          },
